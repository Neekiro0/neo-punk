.MissionsTreeAct {
    height: auto;
    font-size: 32px;
    width: 100%;
    margin-top: 20px;
    margin-bottom: 20px;
    font-size: 32px;
}

.MissionsTreeAct .MissionsTreeMissionButton {
    width: 70%;
    height: 10%;
    margin-top: 10px;
    padding-top: 20px;
    padding-bottom: 20px;
    padding-right: 0;
    padding-left: 0;
    background-color: rgb(131, 144, 255);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

.MissionsTree {
    flex-grow: 1;
    width: 445px;
    justify-content: space-between;
    align-self: stretch;
    flex-direction: column;
    align-items: flex-start;
    overflow: visible;
    flex-wrap: wrap-reverse;
}

.ScrollView {
    width: 100%;
    height: 100%;
    overflow: scroll;
}

.DetailsStatsStat {
    width: 33.33%;
    flex-grow: 1;
    background-color: rgba(126, 142, 224, 0);
    padding: 20px;
    margin: 20px;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
}

.DetailsStatsStatLabel {
    padding-top: 10px;
    padding-right: 10px;
    padding-bottom: 10px;
    padding-left: 10px;
    background-color: rgb(131, 144, 255);
}

.DetailsStatsStatRate {
    background-color: rgb(118, 130, 233);
}

.DetailsStatsStatRateStar {
    width: 35px;
    height: 35px;
    margin: 20px;
}

.DetailsStatsStatBest {
    -unity-text-align: upper-center;
    font-size: 20px;
    padding-top: 10px;
    padding-right: 10px;
    padding-bottom: 10px;
    padding-left: 10px;
    background-color: rgb(131, 144, 255);
}

.custom-scroll .unity-scroll-view__vertical-scroller {
    opacity: 0;
}